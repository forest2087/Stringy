    "string",
    "manipulation",
    "utility",
    "methods",
    "utf-8",
    "helpers",
    "utils",
    "utf"
  ],
  "homepage": "https://github.com/forest2087/Stringy",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel St. Jules",
      "email": "danielst.jules@gmail.com",
      "homepage": "http://www.danielstjules.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/polyfill-mbstring": "~1.1",
    "illuminate/support": ">=5.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "support": {
    "issues": "https://github.com/forest2087/Stringy/issues",
    "source": "https://github.com/forest2087/Stringy"
